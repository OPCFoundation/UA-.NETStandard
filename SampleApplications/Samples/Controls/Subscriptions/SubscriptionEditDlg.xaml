        <Controls:NumericUpDown x:Name="PublishingIntervalNC" HorizontalAlignment="Left" Height="100" Margin="10,84,0,0" VerticalAlignment="Top" Width="100"/>
        <Controls:NumericUpDown x:Name="PriorityNC" HorizontalAlignment="Left" Height="100" Margin="147,84,0,0" VerticalAlignment="Top" Width="100"/>
        <Controls:NumericUpDown x:Name="KeepAliveCountNC" HorizontalAlignment="Left" Height="100" Margin="72,189,0,0" VerticalAlignment="Top" Width="100"/>
        <Controls:NumericUpDown x:Name="MaxNotificationsCTRL" HorizontalAlignment="Left" Height="100" Margin="72,189,0,0" VerticalAlignment="Top" Width="100"/>
        <Controls:NumericUpDown x:Name="LifetimeCountCTRL" HorizontalAlignment="Left" Height="100" Margin="72,189,0,0" VerticalAlignment="Top" Width="100"/>
        <CheckBox x:Name="PublishingEnabledCK" Content="CheckBox" HorizontalAlignment="Left" Margin="10,317,0,0" VerticalAlignment="Top"/>
    </Grid>
</Page>
